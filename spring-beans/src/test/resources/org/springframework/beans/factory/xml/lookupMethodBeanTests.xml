<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">


	<bean id="abstractBean" class="org.springframework.beans.factory.support.AbstractBean" >
		<lookup-method name="get" bean="test"/>
		<lookup-method name="getOneArgument" bean="test"/>
		<lookup-method name="getTwoArguments" bean="test"/>
	</bean>

	<bean id="test" class="org.springframework.tests.sample.beans.TestBean" scope="prototype">
	</bean>
</beans>
